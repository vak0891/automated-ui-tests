{
  "name": "poc-protractor-cucumber",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test-chrome": "protractor-flake --parser cucumber --node-bin node --max-attempts=5 -- config/config-chrome.js",
    "test-firefox": "protractor-flake --parser cucumber --node-bin node --max-attempts=5 -- config/config-firefox.js",
    "test-multi": "protractor-flake --parser cucumber --node-bin node --max-attempts=5 -- config/multiConfig.js",
    "browserstack": "protractor-flake --parser cucumber --node-bin node --max-attempts=5 -- config/browserstack",
    "webdriver-start": "webdriver-manager start",
    "webdriver-update": "webdriver-manager update",
    "lint": "eslint **/*.js --quiet",
    "lint-fix": "eslint **/*.js --fix"
  },
  "pre-commit": [
    "lint"
  ],
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-json-pattern": "^1.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-cucumber": "^1.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-protractor": "^2.1.0",
    "pre-commit": "^1.2.2",
    "protractor-cucumber-framework": "^4.1.1"
  },
  "repository": "",
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cucumber": "^3.1.0",
    "cucumber-html-report": "^0.6.5",
    "cucumber-html-reporter": "^5.0.0",
    "eslint": "^6.0.1",
    "global": "^4.4.0",
    "logger": "0.0.1",
    "mkdirp": "^0.5.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.18.2",
    "protractor": "^5.4.2",
    "protractor-flake": "^4.0.0",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2",
    "uuid": "^3.3.2"
  }
}
